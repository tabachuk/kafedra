    <SolidColorBrush x:Key="FadeForegroundColor">#BB000000</SolidColorBrush>
    <SolidColorBrush x:Key="MouseOverButtonColor">#30000000</SolidColorBrush>
    <SolidColorBrush x:Key="BackgroundColor">WhiteSmoke</SolidColorBrush>
